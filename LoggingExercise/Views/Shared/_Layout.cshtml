<!DOCTYPE html>

<html>
@*<head>
        <meta name="viewport" content="width=device-width" />
        <title>@ViewBag.Title</title>
    </head>*@

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Twitter -->
    <meta name="twitter:site" content=@("@themepixels")>
    <meta name="twitter:creator" content=@("@themepixels")>
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="Amanda">
    <meta name="twitter:description" content="Premium Quality and Responsive UI for Dashboard.">
    <meta name="twitter:image" content="http://themepixels.me/amanda/img/amanda-social.png">

    <!-- Facebook -->
    <meta property="og:url" content="http://themepixels.me/amanda">
    <meta property="og:title" content="Bracket">
    <meta property="og:description" content="Premium Quality and Responsive UI for Dashboard.">

    <meta property="og:image" content="http://themepixels.me/amanda/img/amanda-social.png">
    <meta property="og:image:secure_url" content="http://themepixels.me/amanda/img/amanda-social.png">
    <meta property="og:image:type" content="image/png">
    <meta property="og:image:width" content="1200">
    <meta property="og:image:height" content="600">

    <!-- Meta -->
    <meta name="description" content="Premium Quality and Responsive UI for Dashboard.">
    <meta name="author" content="ThemePixels">


    <title>ReloarHub</title>

    <!-- vendor css -->
    <link href="~/Content/lib/font-awesome/css/font-awesome.css" rel="stylesheet">
    <link href="~/Content/lib/Ionicons/css/ionicons.css" rel="stylesheet">
    <link href="~/Content/lib/perfect-scrollbar/css/perfect-scrollbar.css" rel="stylesheet">
    <link href="~/Content/lib/jquery-toggles/toggles-full.css" rel="stylesheet">
    <link href="~/Content/lib/rickshaw/rickshaw.min.css" rel="stylesheet">

    <!-- Amanda CSS -->
    <link rel="stylesheet" href="~/Content/css/amanda.css">
</head>

<body>

   @* @Html.Partial("_NavBar")*@

    <div>
        @RenderBody()
    </div>
    <script src="~/Content/lib/jquery/jquery.js"></script>
    <script src="~/Content/lib/popper.js/popper.js"></script>
    <script src="~/Content/lib/bootstrap/bootstrap.js"></script>
    <script src="~/Content/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.js"></script>
    <script src="~/Content/lib/jquery-toggles/toggles.min.js"></script>
    <script src="~/Content/lib/d3/d3.js"></script>
    <script src="~/Content/lib/rickshaw/rickshaw.min.js"></script>
    <script src="http://maps.google.com/maps/api/js?key=AIzaSyAEt_DBLTknLexNbTVwbXyq2HSf2UbRBU8"></script>
    <script src="~/Content/lib/gmaps/gmaps.js"></script>
    <script src="~/Content/lib/Flot/jquery.flot.js"></script>
    <script src="~/Content/lib/Flot/jquery.flot.pie.js"></script>
    <script src="~/Content/lib/Flot/jquery.flot.resize.js"></script>
    <script src="~/Content/lib/flot-spline/jquery.flot.spline.js"></script>

    <script src="~/Content/js/amanda.js"></script>
    <script src="~/Content/js/ResizeSensor.js"></script>
    <script src="~/Content/js/dashboard.js"></script>
    @RenderSection("scripts", false);
</body>
</html>
